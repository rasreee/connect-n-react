{
  "name": "clockwise-coding-challenge",
  "version": "1.0.0",
  "description": "A front-end coding challenge for potential clockwise engineers",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rasreee/clockwise-challenge-solution.git"
  },
  "author": "voxmatt",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf ./dist ./tsconfig.tsbuildinfo",
    "build": "yarn clean && react-scripts build",
    "test": "jest --clearCache && jest",
    "test:watch": "jest --watch",
    "test:all": "yarn typecheck && yarn build && yarn test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit --incremental false",
    "format:prettier": "prettier -w .",
    "format": "yarn format:prettier && yarn lint --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "lodash.times": "^4.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash.times": "^4.3.6",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-runner-groups": "^2.1.0",
    "prettier": "^2.6.0",
    "react-scripts": "5.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4"
  },
  "env": {
    "test": {
      "presets": [
        "react-app"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
